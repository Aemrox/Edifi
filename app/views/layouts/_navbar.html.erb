
<ul>
    <li>
      <%= link_to "Home", root_path  %>
    </li>
    <% if logged_in? %>
      <li>  <%= link_to "Log Out", logout_path %> </li>
      <li>  <%= "Welcome, #{current_user.user_name}" %> </li>
    <% else  %>
      <li>  <%= link_to "Log In", login_path %> </li>
      <li>  <%= link_to "Sign Up", new_user_path %> </li>
    <% end %>
</ul>

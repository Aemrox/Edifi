<h1> <%= @user.user_name %> </h1>

<h2> Bio:</h2>
<p>
  <% if @user.bio%>
    <%= @user.bio  %>
  <% else %>
     No Bio :(
  <% end %>
</p>


<% if @user.teacher? %>
  <%= render partial: "teacher_skills", locals: {teacher: @user} %>
<%end%>

<% if logged_in? || @user.id != current_user.id %>

  <%= render "connections" %>

<% end %>